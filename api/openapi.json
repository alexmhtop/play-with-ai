  "components": {
    "schemas": {
      "Book": {
        "properties": {
          "id": {
            "type": "integer",
            "title": "Id"
          },
          "title": {
            "type": "string",
            "maxLength": 200,
            "minLength": 1,
            "title": "Title"
          },
          "author": {
            "type": "string",
            "maxLength": 200,
            "minLength": 1,
            "title": "Author"
          },
          "price": {
            "type": "number",
            "title": "Price"
          },
          "in_stock": {
            "type": "boolean",
            "title": "In Stock",
            "default": true
          },
          "version": {
            "type": "integer",
            "title": "Version",
            "default": 1
          }
        },
        "type": "object",
        "required": [
          "id",
          "title",
          "author",
          "price"
        ],
        "title": "Book"
      },
      "CreateBook": {
        "properties": {
          "title": {
            "type": "string",
            "maxLength": 200,
            "minLength": 1,
            "title": "Title"
          },
          "author": {
            "type": "string",
            "maxLength": 200,
            "minLength": 1,
            "title": "Author"
          },
          "price": {
            "type": "number",
            "exclusiveMaximum": 100000.0,
            "exclusiveMinimum": 0.0,
            "title": "Price"
          },
          "in_stock": {
            "type": "boolean",
            "title": "In Stock",
            "default": true
          }
        },
        "type": "object",
        "required": [
          "title",
          "author",
          "price"
        ],
        "title": "CreateBook"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "UpdateBook": {
        "properties": {
          "title": {
            "anyOf": [
              {
                "type": "string",
                "maxLength": 200,
                "minLength": 1
              },
              {
                "type": "null"
              }
            ],
            "title": "Title"
          },
          "author": {
            "anyOf": [
              {
                "type": "string",
                "maxLength": 200,
                "minLength": 1
              },
              {
                "type": "null"
              }
            ],
            "title": "Author"
          },
          "price": {
            "anyOf": [
              {
                "type": "number",
                "exclusiveMaximum": 100000.0,
                "exclusiveMinimum": 0.0
              },
              {
                "type": "null"
              }
            ],
            "title": "Price"
          },
          "in_stock": {
            "anyOf": [
              {
                "type": "boolean"
              },
              {
                "type": "null"
              }
            ],
            "title": "In Stock"
          }
        },
        "type": "object",
        "title": "UpdateBook"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      }
    },
    "securitySchemes": {
      "HTTPBearer": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  }
